<div class="container">
  <h1 class="text-center">Our cars</h1>
  <div class="text-center">
  </div>
  <% @cars.each do |car| %>
      <div class="card-product bookings-element">
        <%= cl_image_tag car.photo.key, class: "image-card" %>
        <div class="card-product-infos ">
          <div>
            <h2><%= car.description %></h2>
            <p><%= car.info %></p>
            </br>
            </br>
          </div>
          <div>
            <%= link_to "See more", car_url(car), class: "btn-flat" %>
          </div>
        </div>
      </div>
  <% end %>

  </div>
