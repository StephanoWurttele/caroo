
<h1 class="text-center">Our cars</h1>
<div class="text-center">
  <button type="button" class="btn btn-info"><%= link_to "Create new car", new_car_url %></button>
</div>
<br>
<% @cars.each do |car| %>
    <p><%= car.info %></p>
    <%= car.description %>
    <%= link_to "See more", car_url(car) %>
    <br></br>
    <br></br>
<% end %>
