<h1>Car</h1>
<h5><%= @car.show %></h5>
<p><%= @car.price %></p>
<%= link_to "Rent", car_bookings_url(@car), method: :post %>
<h2>Reviews</h2>
<br></br>
<p><%= link_to "Create a Review", new_review_path %><p>
  <% @cars.reviews.each do |review| %>
    <h1><%= review.rating %></h1>
    <p><%= review.content %></p>
  <% end %>


